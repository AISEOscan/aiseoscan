{
  "functions": {
    "pages/api/**/*": {
      "memory": 1024,
      "maxDuration": 60
    }
  },
  "redirects": [
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "www.aiseoscan.dev"
        }
      ],
      "destination": "https://aiseoscan.dev/:path*",
      "permanent": true
    }
  ]
}